{
  "name": "fond-of-spryker/company-users-rest-api",
  "description": "Company users rest api module",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Rose",
      "email": "daniel-rose@gmx.de"
    },
    {
      "name": "Felix Pfister",
      "email": "felix@pfister.de"
    }
  ],
  "scripts": {
    "ci": "make ci",
    "codeception": "make codeception",
    "phpcs": "make phpcs",
    "phpstan": "make phpstan"
  },
  "require": {
    "php": ">=7.3",
    "fond-of-oryx/company-users-rest-api-extension": "^1.1.0",
    "fond-of-spryker/company-user-reference": "dev-master",
    "spryker/company": "^1.4.0",
    "spryker/company-business-unit": "^1.0.0 || ^2.0.0",
    "spryker/company-user": "^2.3.0",
    "spryker/company-role": "^1.0.0",
    "spryker/glue-application": "^1.0.0",
    "spryker/permission": "^1.0.0"
  },
  "require-dev": {
    "fond-of-codeception/spryker": "^1.0",
    "spryker/code-sniffer": "*",
    "spryker-sdk/phpstan-spryker": "*"
  },
  "replace": {
    "spryker/company-users-rest-api": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/",
      "Generated\\": "src/Generated/",
      "Orm\\Zed\\": "src/Orm/Zed/"
    }
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
